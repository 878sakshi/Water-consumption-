#include <stdio.h>

int main ()
{ int principal,rate, time, interest;
printf ("enter the principal");
scanf ("%d \n", principal);

printf ("enter the rate");
scanf ("%d \n",rate);

printf ("enter the time");
scanf ("%d \n",time);

interest= principal*rate*time/100;
printf ("simple interest is %d",interest);

return 0;
}
